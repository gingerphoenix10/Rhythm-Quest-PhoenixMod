  <PropertyGroup>
    <PostBuildEvent>@Echo Off
SETLOCAL EnableExtensions
set EXE=Rhythm Quest Demo.exe
Set "DIR="
For /F "Tokens=1,2*" %25%25A In ('Reg Query HKCU\SOFTWARE\Valve\Steam') Do (
    If "%25%25A" Equ "SteamPath" Set "DIR=%25%25C\steamapps\common\Rhythm Quest Demo\")
If Not Defined DIR echo Couldn't find Rhythm Quest Demo. Cannot auto-install mod. &amp;&amp; Exit/B
If Not Exist "%25DIR%25" echo Couldn't find Rhythm Quest Demo. Cannot auto-install mod. &amp;&amp; Exit/B
set MODS=%25DIR%25\Mods
set DLL=PhoenixMod.dll
FOR /F %25%25x IN ("%25EXE%25") do set EXE_=%25%25x
FOR /F %25%25x IN ('tasklist /NH /FI "IMAGENAME eq %25EXE%25"') DO IF %25%25x == %25EXE_%25 (
  taskkill /F /IM "%25EXE%25"
)
ping 127.0.0.1 -n 2 &gt; nul
if exist "%25MODS%25\%25DLL%25" (
  del "%25MODS%25\%25DLL%25"
)
ping 127.0.0.1 -n 2 &gt; nul
copy "$(TargetPath)" "%25MODS%25"
:loop
if exist "%25MODS%25\%25DLL%25" (
  cd "%25DIR%25"
  ping 127.0.0.1 -n 2 &gt; nul
  start "" /b "%25EXE%25"
  exit /b 0
) else (
  goto loop
)</PostBuildEvent>
  </PropertyGroup>
